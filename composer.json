{
  "name": "xcore/inline-editing-bundle",
  "type": "symfony-bundle",
  "description": "Content inline editable bundle for Symfony.",
  "keywords": [
    "symfony",
    "xcore"
  ],
  "homepage": "https://appio.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/orm": "^2.5",
    "xcore/inline-editing": "^1.1.0",
    "symfony/framework-bundle": "^3.3",
    "symfony/expression-language": "^3.3",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "symfony/security": "^3.3",
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^3.0",
    "phpstan/phpstan": "@dev"
  },
  "suggest": {
    "symfony/security": "If you wanna use default permission checker subscriber"
  },
  "autoload": {
    "psr-4": {
      "XcoreCMS\\InlineEditingBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XcoreCMS\\InlineEditingBundle\\Tests\\": "tests/"
    }
  }
}
